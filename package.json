{
  "name": "solid-quality",
  "type": "module",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "build": "NODE_ENV=production solid-start build",
    "check-updates": "ncu",
    "clean": "rm -rf ./node_modules & rm ./pnpm-lock.yaml",
    "coverage": "NODE_ENV=test node_modules/.bin/vitest run --coverage",
    "coverage:view": "NODE_ENV=test npm run coverage && open ./reports/index.html",
    "db:reset": "dotenv -e .env.development -- prisma db push --force-reset && npx prisma generate && npx prisma db seed",
    "db:studio": "dotenv -e env.development -- prisma studio",
    "dev": "dotenv -e .env.development -- run-p -l dev:*",
    "dev:db": "docker compose -f ./configs/docker/docker-compose.yml up",
    "dev:solid": "dotenv -e .env.development -- solid-start dev",
    "docker:reset": "docker compose -f ./configs/docker/docker-compose.yml down ",
    "format": "run-p -l format:*",
    "format:eslint": "eslint --fix .",
    "format:prettier": "prettier --write . '!**/*.{js,jsx,ts,tsx}'",
    "lint:eslint": "eslint ./src",
    "lint:prettier": "prettier . '!**/*.{js,jsx,ts,tsx}'",
    "playwright:test": "npx playwright test --project=chromium --project=firefox",
    "playwright:debug-test": "npx playwright test --project=chromium --debug --",
    "preview": "vite preview",
    "start": "solid-start start",
    "test": "NODE_ENV=test vitest run",
    "test:watch": "NODE_ENV=test vitest watch",
    "test:ui": "vitest --ui",
    "postinstall": "prisma generate",
    "postbuild": "cp node_modules/prisma/*engine*.node .vercel/output/functions/render.func/ && cp prisma/schema.prisma .vercel/output/functions/render.func/"
  },
  "dependencies": {
    "@auth/core": "^0.4.0",
    "@auth/solid-start": "^0.1.0",
    "@aws-sdk/client-s3": "^3.267.0",
    "@aws-sdk/s3-request-presigner": "^3.267.0",
    "@next-auth/prisma-adapter": "^1.0.5",
    "@prisma/client": "^4.10.1",
    "@solidjs/meta": "^0.28.2",
    "@solidjs/router": "^0.7.0",
    "@suid/icons-material": "^0.5.8",
    "@suid/material": "^0.10.1",
    "@tanstack/solid-query": "4.24.6",
    "@trpc/client": "^10.11.0",
    "@trpc/server": "^10.11.0",
    "axios": "^1.3.2",
    "dayjs": "^1.11.7",
    "node-fetch": "^3.3.0",
    "pusher": "^5.1.2",
    "pusher-js": "^8.0.1",
    "solid-js": "^1.6.10",
    "solid-start": "^0.2.20",
    "solid-start-trpc": "^0.0.16",
    "solid-toast": "^0.4.0",
    "solid-trpc": "^0.0.11-rc.2",
    "superjson": "^1.12.2",
    "undici": "5.18.0",
    "zod": "^3.20.6"
  },
  "devDependencies": {
    "@suid/vite-plugin": "^0.1.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@types/eslint": "^8.21.0",
    "@types/node": "^18.13.0",
    "@types/node-fetch": "^2.6.2",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "@vitest/ui": "^0.28.4",
    "autoprefixer": "^10.4.13",
    "dotenv": "^16.0.3",
    "dotenv-cli": "^7.0.0",
    "eslint": "^8.34.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-solid": "^0.9.4",
    "eslint-plugin-tailwindcss": "^3.8.3",
    "eslint-plugin-vitest-globals": "^1.3.1",
    "husky": "^8.0.3",
    "jsdom": "^21.1.0",
    "lint-staged": "^13.1.1",
    "msw": "^1.0.1",
    "npm-check-updates": "^16.7.4",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.21",
    "prettier": "^2.8.4",
    "prisma": "^4.10.1",
    "rollup-plugin-visualizer": "^5.9.0",
    "solid-start-node": "^0.2.20",
    "solid-start-vercel": "^0.2.20",
    "solid-testing-library": "^0.5.0",
    "tailwindcss": "^3.2.6",
    "tsx": "^3.12.3",
    "typescript": "^4.9.5",
    "vite": "^4.1.1",
    "vite-plugin-checker": "^0.5.5",
    "vite-plugin-environment": "^1.1.3",
    "vite-plugin-solid": "^2.5.0",
    "vite-tsconfig-paths": "^4.0.5",
    "vitest": "^0.28.4"
  },
  "prisma": {
    "seed": "dotenv -e .env.development -- tsx prisma/seed.ts"
  }
}
