{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "node",
    "jsxImportSource": "solid-js",
    "jsx": "preserve",
    "types": ["node", "solid-start/env", "vite/client", "vitest/globals", "@testing-library/jest-dom"],
    "baseUrl": "./src",
    "strict": true,
    "paths": {
      "~/*": ["./*"]
    }
  }
}
